= javascript_include_tag :defaults
= javascript_include_tag 'http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js'
= javascript_include_tag '/javascripts/libs/modernizr-latest.js'
= javascript_include_tag '/javascripts/libs/hammerjs/jquery.hammer.js'
= javascript_include_tag '/javascripts/application.js'

    	 
= javascript_include_tag 'touch.js'

= javascript_include_tag '/javascripts/libs/supersized/slideshow/js/jquery.easing.min.js', '/javascripts/libs/supersized/slideshow/js/supersized.3.2.7.min.js', '/javascripts/libs/supersized-init.js'

// jquery lazyload
= javascript_include_tag '/javascripts/libs/lazyload/jquery.lazyload.min.js'
:javascript
  $(function() {
    $("img").lazyload({
      effect : "fadeIn"
    });
  });

= yield :javascript
